<div class="modal fade" id="loginModal" tabindex="-1" role="dialog" aria-labelledby="loginModal" aria-hidden="true">
	<div class="modal-dialog">
		<div class="modal-content">
			<div class="modal-header">
				<h4 class="modal-title" id="myModalLabel">
							登入
						</h4>
			</div>
			<div class="modal-body">
				<form>
					<div class="form-group">
						<label for="log_name">用户名：</label>
						<input type="text" class="form-control" id="log_name" placeholder="">
					</div>
					<div class="form-group">
						<label for="log_passward">密码：</label>
						<input type="password" class="form-control" id="log_passward" placeholder="">
					</div>
				</form>
				<p class="errMsg" id="login_msg"></p>
			</div>
			<div class="modal-footer">
				<button type="button" class="btn btn-default" data-dismiss="modal">关闭</button>
				<button type="button" class="btn btn-primary" id="login">登入</button>
			</div>
		</div>
	</div>
</div>